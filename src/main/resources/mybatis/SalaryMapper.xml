<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.green.when.mapper.SalaryMapper">
    <select id="getSalary" resultType="SalaryVo">
        SELECT * FROM member_tb;
    </select>

    <insert id="signup">
        INSERT INTO member_tb(userid, usernickname, userpw, useremail)
        VALUES (#{userid}, #{usernickname}, #{userpw}, #{useremail})
    </insert>
</mapper>